{
    "name": "amethyst/data-builder",
    "keywords": ["laravel", "amethyst", "amethyst-data-builder", "data-builder"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "amethyst/core": "0.2.*",
        "railken/template": "^1.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "*",
        "amethyst/foo": "0.2.*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amethyst\\Providers\\DataBuilderServiceProvider"
            ]
        }
    }
}
